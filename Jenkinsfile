pipeline {
    agent any

    stages {
        stage('One') {
            steps {
                echo 'Hello welcome to pipeline demo'
            }
        }
        stage('Two') {
            steps {
                echo 'Sample Testing Stage 2'
            }
        }
        stage('Three') {
            steps {
                echo 'Thanks for using Jenkins'
            }
        }
    }
}
